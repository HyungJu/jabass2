{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["airbnb-base"],
  "parserOptions": {
    "ecmaVersion": 13,
    "sourceType": "module"
  },
  "rules": {
    "no-undef": "off",
    "import/extensions": "off",
    "no-confusing-arrow": "off",
    "implicit-arrow-linebreak": "off",
    "no-use-before-define": ["error", "nofunc"]
  },
  "plugins": ["cypress"],
  "ignorePatterns": [
    "cypress/fixtures/*",
    "cypress/plugins/*",
    "cypress/support/*",
    "dist"
  ]
}
